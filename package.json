    "keytar": "^7.9.0"
  },
  "devDependencies": {
    "concurrently": "^8.0.0",
    "electron": "^29.0.0",
    "electron-builder": "^24.0.0",
    "tailwindcss": "^3.4.0",
    "postcss": "^8.4.0",
    "autoprefixer": "^10.4.0",
    "jest": "^29.7.0"
  },
  "license": "UNLICENSED",
  "jest": {
    "testEnvironment": "node"
  }
